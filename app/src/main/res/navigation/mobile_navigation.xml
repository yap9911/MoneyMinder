    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="my.edu.utar.moneyminder.ui.transaction.TransactionFragment"
        android:label="@string/title_transaction"
        tools:layout="@layout/fragment_transaction" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="my.edu.utar.moneyminder.ui.VirtualAdvisor.VirtualAdvisorFragment"
